<div class="col-lg-12 col-md-12">
  <div class="row h-padding v-padding large">
    <div class="col-12">
      <div class="d-flex justify-content-between">
        <div class="p-2">
          <h3>My Tasks</h3>
        </div>
        <div class="p-2">
          <%= link_to 'Print Tasks', print_tasks_url, class: 'btn btn-success text-white'%>
        </div>
      </div>

      <p><%= has_tasks?(@jobs) %></p>

    </div>
  </div>
  <%= render partial: 'job', collection: @jobs, as: 'job' %>
</div>
